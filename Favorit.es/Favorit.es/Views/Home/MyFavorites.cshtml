@model List<Favorit.es.Models.AddFavoriteViewModel>
@*dont forget to declare the view's model on the first line!*@

@{
    ViewBag.Title = "My Favorites";
}

<div class="jumbotron">
    <h1>My Favorit.es</h1>
    <p class="lead">Click to unfavorite something</p>
    <p><a href="@Url.Action("Index", "Home")" class="btn btn-primary btn-lg">Back to Favorit.es</a></p>
</div>

<div class="row">
    @*the users favorites will go here*@
    @foreach (var item in Model)
    {
        <div class="col-md-4">
            <a href="@Url.Action("unfavorite", "home", new { id = item.id })">
                <img class="img-thumbnail" src="@item.url" />
                <br />
                @item.title
            </a>
        </div>
    }
</div>